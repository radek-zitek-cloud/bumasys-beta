{
  "app": {
    "name": "Bumasys Frontend",
    "version": "1.0.0",
    "theme": "default"
  },
  "api": {
    "baseUrl": "http://localhost:4000",
    "graphqlEndpoint": "/graphql",
    "timeout": 10000
  },
  "ui": {
    "theme": {
      "dark": false,
      "primaryColor": "#1976d2"
    },
    "pagination": {
      "defaultPageSize": 10,
      "pageSizeOptions": [5, 10, 25, 50]
    },
    "table": {
      "sortable": true,
      "filterable": true
    }
  },
  "features": {
    "debugMode": false,
    "showConfigCard": true,
    "enableLogging": true
  },
  "logging": {
    "level": "info",
    "console": {
      "enabled": true,
      "pretty": true
    },
    "betterStack": {
      "enabled": false,
      "sourceToken": "",
      "endpoint": ""
    }
  }
}
